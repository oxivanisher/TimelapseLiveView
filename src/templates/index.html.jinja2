<!doctype html>
<html>
<head>
<title>Live view index</title>
</head>
<body>

<h1>Index of cameras</h1>
<ul>
{% for camera in cameras %}
<li><a href="{{ url_for('viewer', camera=camera) }}?t={{ now }}">{{ camera }}</a>
{% else %}
<li><em>Unbelievable. No entries here so far</em>
{% endfor %}
</ul>

</body>
</html>
